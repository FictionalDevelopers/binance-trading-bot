{
    "name": "server",
    "version": "1.0.0",
    "main": "dist/index.js",
    "license": "UNLICENSED",
    "publishConfig": {
        "access": "restricted"
    },
    "scripts": {
        "start": "babel-node src/index.js",
        "build": "rm -rf ./dist && babel src --out-dir dist",
        "lint": "eslint --fix src/*"
    },
    "husky": {
        "hooks": {
            "pre-commit": "if git-branch-is master; then echo 'Current branch is protected from commit. You can not commit directly to master'; exit 1; else lint-staged && lint-diff HEAD; fi",
            "pre-push": "if git-branch-is master; then echo 'Current branch is protected from push.'; exit 1; fi"
        }
    },
    "lint-staged": {
        "*.{js}": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "lodash": "^4.17.15",
        "mongoose": "^5.9.15",
        "node-binance-api": "^0.11.4",
        "rxjs": "^6.5.5",
        "ws": "^7.3.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/node": "^7.8.7",
        "@babel/polyfill": "^7.8.7",
        "@babel/preset-env": "^7.9.6",
        "babel-eslint": "^10.0.3",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-prettier": "^3.0.0",
        "git-branch-is": "^3.1.0",
        "husky": "^4.0.1",
        "lint-diff": "^1.2.1",
        "lint-staged": "^10.0.7",
        "prettier": "^1.19.1",
        "prettier-eslint": "^9.0.1",
        "standard": "^14.3.1"
    }
}
